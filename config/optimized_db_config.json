{
  "postgresql": {
    "min_connections": 5,
    "max_connections": 25,
    "max_overflow": 15,
    "pool_timeout": 20,
    "pool_recycle": 1800,
    "pool_pre_ping": true,
    "command_timeout": 30
  },
  "redis": {
    "max_connections": 50,
    "socket_timeout": 5,
    "socket_connect_timeout": 5,
    "retry_on_timeout": true,
    "health_check_interval": 30
  }
}