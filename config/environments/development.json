  "services": {
    "auth": {
      "name": "Authentication Service",
      "port": 8000,
      "host": "localhost",
      "health_path": "/health",
      "debug": true
    },
    "ac": {
      "name": "Constitutional AI Service", 
      "port": 8001,
      "host": "localhost",
      "health_path": "/health",
      "debug": true
    },
    "integrity": {
      "name": "Integrity Service",
      "port": 8002,
      "host": "localhost", 
      "health_path": "/health",
      "debug": true
    },
    "fv": {
      "name": "Formal Verification Service",
      "port": 8003,
      "host": "localhost",
      "health_path": "/health",
      "debug": true
    },
    "gs": {
      "name": "Governance Synthesis Service",
      "port": 8004,
      "host": "localhost",
      "health_path": "/health",
      "debug": true
    },
    "pgc": {
      "name": "Policy Governance Service",
      "port": 8005,
      "host": "localhost",
      "health_path": "/health",
      "debug": true
    },
    "ec": {
