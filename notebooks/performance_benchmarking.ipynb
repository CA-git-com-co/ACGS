    "## 2. Service Health Check and Baseline Measurement"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Perform health checks for all ACGS services\n",
    "async def check_service_health(service_id, port, timeout=5):\n",
    "    \"\"\"Check health status of an ACGS service.\"\"\"\n",
    "    url = f\"http://localhost:{port}/health\"\n",
    "    \n",
    "    try:\n",
    "        start_time = time.time()\n",
    "        async with aiohttp.ClientSession(timeout=aiohttp.ClientTimeout(total=timeout)) as session:\n",
    "            async with session.get(url) as response:\n",
    "                response_time = (time.time() - start_time) * 1000  # Convert to ms\n",
    "                status = response.status\n",
    "                \n",
    "                return {\n",
    "                    'service_id': service_id,\n",
    "                    'port': port,\n",
    "                    'status_code': status,\n",
    "                    'response_time_ms': response_time,\n",
    "                    'healthy': status == 200,\n",
    "                    'timestamp': datetime.now().isoformat()\n",
    "                }\n",
    "    except Exception as e:\n",
    "        return {\n",
    "            'service_id': service_id,\n",
    "            'port': port,\n",
    "            'status_code': 0,\n",
    "            'response_time_ms': timeout * 1000,\n",
    "            'healthy': False,\n",
    "            'error': str(e),\n",
    "            'timestamp': datetime.now().isoformat()\n",
    "        }\n",
    "\n",
    "async def perform_health_checks():\n",
    "    \"\"\"Perform health checks for all ACGS services.\"\"\"\n",
    "    print(\"üîç Performing health checks for all ACGS services...\")\n",
    "    \n",
    "    tasks = []\n",
    "    for service_id, config in ACGS_SERVICES.items():\n",
    "        task = check_service_health(service_id, config['port'])\n",
    "        tasks.append(task)\n",
    "    \n",
    "    results = await asyncio.gather(*tasks)\n",
    "    \n",
    "    # Display results\n",
    "    healthy_services = sum(1 for r in results if r['healthy'])\n",
    "    total_services = len(results)\n",
    "    \n",
    "    print(f\"\\nüìä Health Check Results:\")\n",
    "    print(f\"  Healthy Services: {healthy_services}/{total_services}\")\n",
    "    print(f\"  System Availability: {(healthy_services/total_services)*100:.1f}%\")\n",
    "    \n",
    "    for result in results:\n",
    "        status_icon = \"‚úÖ\" if result['healthy'] else \"‚ùå\"\n",
    "        service_name = ACGS_SERVICES[result['service_id']]['name']\n",
    "        print(f\"  {status_icon} {service_name} (:{result['port']}) - {result['response_time_ms']:.1f}ms\")\n",
    "    \n",
    "    return results\n",
    "\n",
    "# Run health checks\n",
    "health_results = await perform_health_checks()"
   ]
