    "## 3. Load Testing and Performance Measurement"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Perform load testing on ACGS services\n",
    "async def load_test_service(service_id, port, num_requests=100, concurrency=10):\n",
    "    \"\"\"Perform load testing on a specific service.\"\"\"\n",
    "    url = f\"http://localhost:{port}/health\"\n",
    "    results = []\n",
    "    \n",
    "    async def make_request(session, request_id):\n",
    "        try:\n",
    "            start_time = time.time()\n",
    "            async with session.get(url) as response:\n",
    "                response_time = (time.time() - start_time) * 1000\n",
    "                return {\n",
    "                    'request_id': request_id,\n",
    "                    'response_time_ms': response_time,\n",
    "                    'status_code': response.status,\n",
    "                    'success': response.status == 200\n",
    "                }\n",
    "        except Exception as e:\n",
    "            return {\n",
    "                'request_id': request_id,\n",
    "                'response_time_ms': 5000,  # Timeout\n",
    "                'status_code': 0,\n",
    "                'success': False,\n",
    "                'error': str(e)\n",
    "            }\n",
    "    \n",
    "    # Create semaphore for concurrency control\n",
    "    semaphore = asyncio.Semaphore(concurrency)\n",
    "    \n",
    "    async def bounded_request(session, request_id):\n",
    "        async with semaphore:\n",
    "            return await make_request(session, request_id)\n",
    "    \n",
    "    # Perform load test\n",
    "    start_time = time.time()\n",
    "    \n",
    "    async with aiohttp.ClientSession() as session:\n",
    "        tasks = [bounded_request(session, i) for i in range(num_requests)]\n",
    "        results = await asyncio.gather(*tasks)\n",
    "    \n",
    "    total_time = time.time() - start_time\n",
    "    \n",
    "    # Calculate metrics\n",
    "    successful_requests = sum(1 for r in results if r['success'])\n",
    "    response_times = [r['response_time_ms'] for r in results if r['success']]\n",
    "    \n",
    "    metrics = {\n",
    "        'service_id': service_id,\n",
    "        'total_requests': num_requests,\n",
    "        'successful_requests': successful_requests,\n",
    "        'error_rate_percent': ((num_requests - successful_requests) / num_requests) * 100,\n",
    "        'total_time_seconds': total_time,\n",
    "        'throughput_rps': num_requests / total_time,\n",
    "        'avg_response_time_ms': statistics.mean(response_times) if response_times else 0,\n",
    "        'p50_response_time_ms': statistics.median(response_times) if response_times else 0,\n",
    "        'p95_response_time_ms': np.percentile(response_times, 95) if response_times else 0,\n",
    "        'p99_response_time_ms': np.percentile(response_times, 99) if response_times else 0,\n",
    "        'min_response_time_ms': min(response_times) if response_times else 0,\n",
    "        'max_response_time_ms': max(response_times) if response_times else 0\n",
    "    }\n",
    "    \n",
    "    return metrics, results\n",
    "\n",
    "async def perform_load_testing():\n",
    "    \"\"\"Perform load testing on all healthy ACGS services.\"\"\"\n",
    "    print(\"üöÄ Starting load testing on ACGS services...\")\n",
    "    \n",
    "    load_test_results = {}\n",
    "    \n",
    "    for service_id, config in ACGS_SERVICES.items():\n",
    "        # Only test healthy services\n",
    "        service_health = next((r for r in health_results if r['service_id'] == service_id), None)\n",
    "        \n",
    "        if service_health and service_health['healthy']:\n",
    "            print(f\"\\nüìä Load testing {config['name']} (:{config['port']})...\")\n",
    "            \n",
    "            metrics, detailed_results = await load_test_service(\n",
    "                service_id, config['port'], num_requests=50, concurrency=5\n",
    "            )\n",
    "            \n",
    "            load_test_results[service_id] = {\n",
    "                'metrics': metrics,\n",
    "                'detailed_results': detailed_results\n",
    "            }\n",
    "            \n",
    "            # Display results\n",
    "            print(f\"  ‚úÖ Completed: {metrics['successful_requests']}/{metrics['total_requests']} requests\")\n",
    "            print(f\"  üìà Throughput: {metrics['throughput_rps']:.1f} RPS\")\n",
    "            print(f\"  ‚è±Ô∏è Avg Response Time: {metrics['avg_response_time_ms']:.1f}ms\")\n",
    "            print(f\"  üìä P95 Response Time: {metrics['p95_response_time_ms']:.1f}ms\")\n",
    "            print(f\"  ‚ùå Error Rate: {metrics['error_rate_percent']:.1f}%\")\n",
    "        else:\n",
    "            print(f\"\\n‚ö†Ô∏è Skipping {config['name']} - Service not healthy\")\n",
    "    \n",
    "    return load_test_results\n",
    "\n",
    "# Run load testing\n",
    "load_test_results = await perform_load_testing()"
   ]
  }
