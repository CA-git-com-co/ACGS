{
  "dashboard": {
    "id": null,
    "title": "ACGS-2 Policy Governance Compiler - Enterprise Monitoring",
    "tags": ["acgs-2", "policy-governance", "enterprise", "constitutional-compliance"],
    "style": "dark",
    "timezone": "UTC",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "annotations": {
      "list": [
        {
          "name": "Constitutional Hash",
          "datasource": "prometheus",
          "enable": true,
          "expr": "acgs_system_info{constitutional_hash=\"cdd01ef066bc6cf2\"}",
          "iconColor": "green",
          "tags": ["constitutional-compliance"]
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "SLA Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "acgs_rps_current{service=\"pgc\"}",
            "legendFormat": "Current RPS",
            "refId": "A"
          },
          {
            "expr": "acgs_uptime_percentage{service=\"pgc\"}",
            "legendFormat": "Uptime %",
            "refId": "B"
          },
          {
            "expr": "acgs_constitutional_compliance_rate{service=\"pgc\"} * 100",
            "legendFormat": "Constitutional Compliance %",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.95, acgs_request_duration_seconds{service=\"pgc\"}) * 1000",
            "legendFormat": "P95 Response Time (ms)",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 1000},
                {"color": "green", "value": 1247}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Uptime %"},
              "properties": [
                {"id": "unit", "value": "percent"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "red", "value": 0},
                    {"color": "yellow", "value": 99.0},
                    {"color": "green", "value": 99.9}
                  ]
                }}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Constitutional Compliance %"},
              "properties": [
                {"id": "unit", "value": "percent"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "red", "value": 0},
                    {"color": "red", "value": 99.9},
                    {"color": "green", "value": 100}
                  ]
                }}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "P95 Response Time (ms)"},
              "properties": [
                {"id": "unit", "value": "ms"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": 0},
                    {"color": "yellow", "value": 5},
                    {"color": "red", "value": 10}
                  ]
                }}
              ]
            }
          ]
        }
      },
      {
        "id": 2,
        "title": "Request Rate (RPS)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "acgs_rps_current{service=\"pgc\"}",
            "legendFormat": "Current RPS",
            "refId": "A"
          },
          {
            "expr": "1247",
            "legendFormat": "Target RPS (1247)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Requests per Second",
            "min": 0,
            "max": 2000
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [1000], "type": "lt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "30s",
          "handler": 1,
          "name": "RPS Below Target",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 3,
        "title": "Response Time Distribution",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, acgs_request_duration_seconds{service=\"pgc\"}) * 1000",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, acgs_request_duration_seconds{service=\"pgc\"}) * 1000",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, acgs_request_duration_seconds{service=\"pgc\"}) * 1000",
            "legendFormat": "P99",
            "refId": "C"
          },
          {
            "expr": "5",
            "legendFormat": "Target P95 (5ms)",
            "refId": "D"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (ms)",
            "min": 0,
            "max": 20
          }
        ]
      },
      {
        "id": 4,
        "title": "Constitutional Compliance Rate",
        "type": "singlestat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "acgs_constitutional_compliance_rate{service=\"pgc\"} * 100",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "percent",
        "thresholds": "99.9,100",
        "colorBackground": true,
        "colors": ["red", "yellow", "green"],
        "sparkline": {
          "show": true,
          "full": false,
          "lineColor": "rgb(31, 120, 193)",
          "fillColor": "rgba(31, 118, 189, 0.18)"
        }
      },
      {
        "id": 5,
        "title": "Service Uptime",
        "type": "singlestat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "acgs_uptime_percentage{service=\"pgc\"}",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "percent",
        "thresholds": "99.0,99.9",
        "colorBackground": true,
        "colors": ["red", "yellow", "green"]
      },
      {
        "id": 6,
        "title": "HPA Replicas",
        "type": "graph",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "kube_horizontalpodautoscaler_status_current_replicas{horizontalpodautoscaler=\"pgc-hpa\"}",
            "legendFormat": "Current Replicas",
            "refId": "A"
          },
          {
            "expr": "kube_horizontalpodautoscaler_status_desired_replicas{horizontalpodautoscaler=\"pgc-hpa\"}",
            "legendFormat": "Desired Replicas",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Replica Count",
            "min": 0,
            "max": 25
          }
        ]
      },
      {
        "id": 7,
        "title": "Resource Utilization",
        "type": "graph",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 16},
        "targets": [
          {
            "expr": "avg(acgs_cpu_utilization_percentage{service=\"pgc\"})",
            "legendFormat": "CPU %",
            "refId": "A"
          },
          {
            "expr": "avg(acgs_memory_utilization_percentage{service=\"pgc\"})",
            "legendFormat": "Memory %",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Utilization %",
            "min": 0,
            "max": 100
          }
        ]
      },
      {
        "id": 8,
        "title": "Business Metrics",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(acgs_rules_generated_total{service=\"pgc\"}[5m])",
            "legendFormat": "Rules Generated/sec",
            "refId": "A"
          },
          {
            "expr": "rate(acgs_human_reviews_required_total{service=\"pgc\"}[5m])",
            "legendFormat": "Human Reviews Required/sec",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Events per Second",
            "min": 0
          }
        ]
      },
      {
        "id": 9,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "rate(acgs_requests_total{service=\"pgc\",status=~\"2..\"}[5m])",
            "legendFormat": "2xx Success",
            "refId": "A"
          },
          {
            "expr": "rate(acgs_requests_total{service=\"pgc\",status=~\"4..\"}[5m])",
            "legendFormat": "4xx Client Error",
            "refId": "B"
          },
          {
            "expr": "rate(acgs_requests_total{service=\"pgc\",status=~\"5..\"}[5m])",
            "legendFormat": "5xx Server Error",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Requests per Second",
            "min": 0
          }
        ]
      }
    ],
