          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 11,
        "title": "Compliance Violations Detected",
        "type": "stat",
        "targets": [
          {
            "expr": "acgs_compliance_violations_detected{workflow_type=\"constitutional_compliance\"}",
            "legendFormat": "Violations"
          }
        ],
        "fieldConfig": {
          "defaults": {
