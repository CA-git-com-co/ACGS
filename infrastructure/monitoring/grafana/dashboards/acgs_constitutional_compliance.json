{
  "dashboard": {
    "id": null,
    "title": "ACGS Constitutional Compliance Dashboard",
    "tags": [
      "acgs",
      "constitutional",
      "compliance",
      "monitoring"
    ],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ]
    },
    "refresh": "30s",
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "allValue": null,
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "datasource": "Prometheus",
          "definition": "label_values(acgs_service_info, service)",
          "hide": 0,
          "includeAll": true,
          "label": "Service",
          "multi": true,
          "name": "service",
          "options": [],
          "query": "label_values(acgs_service_info, service)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "tagValuesQuery": "",
          "tags": [],
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Constitutional Compliance Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(acgs_constitutional_compliance_score{service=~\"$service\"})",
            "format": "time_series",
            "intervalFactor": 1,
            "legendFormat": "Overall Compliance Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.8
                },
                {
                  "color": "green",
                  "value": 0.9
                }
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "textMode": "auto",
          "colorMode": "background"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Constitutional Hash Validation",
        "type": "stat",
        "targets": [
          {
            "expr": "count(acgs_service_info{constitutional_hash=\"cdd01ef066bc6cf2\", service=~\"$service\"})",
            "format": "time_series",
            "intervalFactor": 1,
            "legendFormat": "Services with Valid Hash",
            "refId": "A"
          },
          {
            "expr": "count(acgs_service_info{service=~\"$service\"})",
            "format": "time_series",
            "intervalFactor": 1,
            "legendFormat": "Total Services",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "textMode": "value_and_name",
          "colorMode": "background"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Performance Targets Status",
        "type": "table",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(acgs_request_duration_seconds_bucket{service=~\"$service\"}[5m])) * 1000",
            "format": "table",
            "instant": true,
            "intervalFactor": 1,
            "legendFormat": "P99 Latency (ms)",
            "refId": "A"
          },
          {
            "expr": "rate(acgs_requests_total{service=~\"$service\"}[5m])",
            "format": "table",
            "instant": true,
            "intervalFactor": 1,
            "legendFormat": "Throughput (RPS)",
            "refId": "B"
          },
          {
            "expr": "rate(acgs_cache_hits_total{service=~\"$service\"}[5m]) / (rate(acgs_cache_hits_total{service=~\"$service\"}[5m]) + rate(acgs_cache_misses_total{service=~\"$service\"}[5m]))",
            "format": "table",
            "instant": true,
            "intervalFactor": 1,
            "legendFormat": "Cache Hit Rate",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "P99 Latency (ms)"
              },
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "green",
                        "value": null
                      },
                      {
                        "color": "yellow",
                        "value": 3
                      },
                      {
                        "color": "red",
                        "value": 5
                      }
                    ]
                  }
                },
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Throughput (RPS)"
              },
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "red",
                        "value": null
                      },
                      {
                        "color": "yellow",
                        "value": 80
                      },
                      {
                        "color": "green",
                        "value": 100
                      }
                    ]
                  }
                },
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Cache Hit Rate"
              },
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "red",
                        "value": null
                      },
                      {
                        "color": "yellow",
                        "value": 0.8
                      },
                      {
                        "color": "green",
                        "value": 0.85
                      }
                    ]
                  }
                },
                {
                  "id": "unit",
                  "value": "percentunit"
                },
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Constitutional Violations Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(acgs_constitutional_violations_total{service=~\"$service\"}[5m])",
            "format": "time_series",
            "intervalFactor": 1,
            "legendFormat": "{{service}} - {{violation_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Violations",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "xAxis": {
          "show": true
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [
                  0
                ],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": [
                  "A",
                  "5m",
                  "now"
                ]
              },
              "reducer": {
                "params": [],
                "type": "last"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "0m",
          "frequency": "10s",
          "handler": 1,
          "name": "Constitutional Violations Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 5,
        "title": "HITL Oversight Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(acgs_hitl_oversight_requests_total{service=~\"$service\"}[5m])",
            "format": "time_series",
            "intervalFactor": 1,
            "legendFormat": "{{service}} - Oversight Requests/sec",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(acgs_hitl_decision_duration_seconds_bucket{service=~\"$service\"}[5m])) * 1000",
            "format": "time_series",
            "intervalFactor": 1,
            "legendFormat": "{{service}} - P99 Decision Latency (ms)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Rate / Latency",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Service Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=~\"acgs-.*\", service=~\"$service\"}",
            "format": "time_series",
            "intervalFactor": 1,
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN"
                  },
                  "1": {
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "textMode": "value_and_name",
          "colorMode": "background"
        },
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 24
        }
      }
    ]
  },
  "overwrite": true,
  "constitutional_hash": "cdd01ef066bc6cf2"
}