{
  "dashboard": {
    "id": null,
    "title": "ACGS Enterprise Monitoring Dashboard",
    "tags": ["acgs", "enterprise", "performance", "constitutional", "governance"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "10s",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "ðŸŽ¯ Enterprise Performance Targets",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(acgs_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99 Latency (ms)",
            "refId": "A"
          },
          {
            "expr": "acgs_cache_hit_rate_percent",
            "legendFormat": "Cache Hit Rate (%)",
            "refId": "B"
          },
          {
            "expr": "acgs_constitutional_compliance_score",
            "legendFormat": "Constitutional Compliance",
            "refId": "C"
          },
          {
            "expr": "rate(acgs_requests_total[5m])",
            "legendFormat": "Throughput (RPS)",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "P99 Latency (ms)"},
              "properties": [
                {"id": "unit", "value": "ms"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": 0},
                    {"color": "yellow", "value": 3},
                    {"color": "red", "value": 5}
                  ]
                }}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Cache Hit Rate (%)"},
              "properties": [
                {"id": "unit", "value": "percent"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "red", "value": 0},
                    {"color": "yellow", "value": 80},
                    {"color": "green", "value": 85}
                  ]
                }}
              ]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "ðŸ“Š P99 Latency Trend (Target: <5ms)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(acgs_request_duration_seconds_bucket{service=~\".*\"}[5m])) * 1000",
            "legendFormat": "{{service}} P99",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "min": 0,
            "max": 10,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 3},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"},
          "tooltip": {"mode": "multi", "sort": "none"}
        }
      },
      {
        "id": 3,
        "title": "ðŸ’¾ Cache Hit Rate (Target: >85%)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "acgs_cache_hit_rate_percent{service=~\".*\"}",
            "legendFormat": "{{service}} Cache Hit Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 85}
              ]
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"},
          "tooltip": {"mode": "multi", "sort": "none"}
        }
      },
      {
        "id": 4,
        "title": "ðŸ›ï¸ Constitutional Compliance Status",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "acgs_constitutional_compliance_score",
            "legendFormat": "Compliance Score",
            "refId": "A"
          },
          {
            "expr": "acgs_constitutional_hash_valid{hash=\"cdd01ef066bc6cf2\"}",
            "legendFormat": "Hash Valid",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 3,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.95},
                {"color": "green", "value": 0.99}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "vertical"
        }
      },
      {
        "id": 5,
        "title": "ðŸš€ Service Throughput (Target: >100 RPS)",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(acgs_requests_total[5m])) by (service)",
            "legendFormat": "{{service}} RPS",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 100}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "âš¡ System Health Overview",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 16},
        "targets": [
          {
            "expr": "up{job=~\"acgs.*\"}",
            "legendFormat": "{{job}} Status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "DOWN", "color": "red"}}, "type": "value"},
              {"options": {"1": {"text": "UP", "color": "green"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "orientation": "vertical"
        }
      },
      {
        "id": 7,
        "title": "ðŸ“ˆ Error Rate Monitoring",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "rate(acgs_requests_total{status=~\"5..\"}[5m]) / rate(acgs_requests_total[5m]) * 100",
            "legendFormat": "{{service}} Error Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "ðŸ”„ WINA Optimization Metrics",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "expr": "acgs_wina_optimization_score",
            "legendFormat": "WINA Score",
            "refId": "A"
          },
          {
            "expr": "rate(acgs_wina_optimizations_total[5m])",
            "legendFormat": "Optimizations/sec",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "enable": true,
          "expr": "acgs_deployment_timestamp",
          "iconColor": "blue",
          "titleFormat": "Deployment",
          "textFormat": "{{service}} deployed"
        },
        {
          "name": "Alerts",
          "datasource": "prometheus", 
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\"}",
          "iconColor": "red",
          "titleFormat": "Alert: {{alertname}}",
          "textFormat": "{{summary}}"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(acgs_requests_total, service)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        },
        {
          "name": "environment",
          "type": "query", 
          "query": "label_values(acgs_requests_total, environment)",
          "refresh": 1,
          "includeAll": false,
          "multi": false
        }
      ]
    }
  }
}
